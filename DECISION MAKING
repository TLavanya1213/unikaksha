#Budget
A=int(input())
B=int(input())
C=int(input())
D=int(input())
E=int(input())
cost=((A*350.34)+(B*230.90)+(C*190.55)+(D*125.30)+(E*180.90))
if cost<15000:
    print("yes")
else:
    print("no")
    
    
#Sece Dining
E=input()
R=int(input())
if E=="front":
    if R==1:
        print("Left Handed")
    elif R==2:
        print("Right Handed")
    else:
        print("Invalid Input")
elif E=="rear":
    if R==1:
        print("Right Handed")
    elif R==2:
        print("Left Handed")
    else:
        print("Invalid Input")
else:
    print("Invalid Input")
    
    
#Intersection of a Circle
x1=int(input())
y1=int(input())
r1=int(input())
x2=int(input())
y2=int(input())
r2=int(input())
x=x1-x2
y=y1-y2
c1c2=((x**2)+(y**2))**0.5
if c1c2==r1+r2:
    print("The circles are tangential")
elif c1c2<r1+r2:
    print("The circles overlap")
else:
    print("The circles doesnot overlap")
    
    
    
#Microwave Oven
print("Enter the number of items")
n=int(input())
print("Enter the single item heating time")
t=float(input())
if n==1:
    print("The recommended heating time is %.2f"%t)
elif n==2:
    print("The recommended heating time is %.2f"%(1.5*t))
elif n==3:
    print("The recommended heating time is %.2f"%(2*t))
else:
    print("Number of items is more")
    
    
    
